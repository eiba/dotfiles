* {
    padding: 5px 10px;
    /*background-color: #414141;*/
    background-color: #383c4a;
    color: #fff;
    font: "Font Awesome 5 Free 45";
}

#window {
  width: 700px;
  height: 250px;
  border-radius: 5px;
  children: [ horibox ];
}
#horibox {
  orientation: horizontal;
  children: [ listview ];
}
#listview {
  layout: horizontal;
}
#element {
  padding: 80px 75px 0px ;
}
#element selected {
  text-color: #81a1c1;
}
